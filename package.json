{
  "name": "@akashic-extension/akashic-box2d",
  "version": "2.0.0",
  "description": "Box2D physics engine for Akashic",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build && npm run doc",
    "clean": "rm -rf lib && rm -rf doc",
    "build": "tsc",
    "doc": "typedoc --out ./doc --mode file && cp -r ./img ./doc",
    "lint": "tslint -c tslint.json src/**/*.ts spec/src/**/*.ts",
    "test": "npm run build && jasmine ./spec/**/*.js && npm run lint"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-box2d.git"
  },
  "publishConfig": {
    "@akashic-extension:registry": "http://registry.npmjs.org/"
  },
  "files": [
    "lib",
    "typings",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "~7.0.10",
    "jasmine": "~2.5.2",
    "tslint": "~4.5.0",
    "typedoc": "0.5.5",
    "typescript": "~2.6.2"
  },
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@akashic/akashic-engine": "~2.0.0",
    "box2dweb": "2.1.0-b"
  }
}
